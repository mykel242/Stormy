package-as: Stormy

ignore:
  - .git
  - .github
  - .gitignore
  - .luacheckrc
  - .busted
  - .release
  - .pkgmeta
  - tests
  - "*.md"
  - Rockspec
  - "*.yml"
  - "*.yaml"
  - ".*.swp"
  - ".*.swo"
  - "*.bak"
  - "*~"
  - ".DS_Store"
  - "Thumbs.db"

# Uncomment to enable CurseForge/WoWInterface auto-packaging
# enable-nolib-creation: no

